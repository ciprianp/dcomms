@model Dcomms.UserApp.LocalUser

@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3>Create new account</h3>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
